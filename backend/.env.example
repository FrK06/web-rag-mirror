# Copy this file to .env and fill in your values
# This file contains all environment variables needed for the RAG system

# API Keys
OPENAI_API_KEY=your-openai-api-key-here
GOOGLE_API_KEY=your-google-api-key-here
GOOGLE_CSE_ID=your-google-custom-search-engine-id

# Twilio Settings
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890
TWIML_URL=https://your-twiml-url.com/voice.xml

# Database Credentials
MONGO_PASSWORD=secure-password-here

# Service Configuration
MAX_LLM_REQUESTS=500
MAX_SMS_PER_DAY=50
MAX_CALLS_PER_DAY=20
RECIPIENT_SMS_LIMIT=5
RECIPIENT_CALL_LIMIT=3

# Cache Settings
CACHE_TTL=3600